  "scripts": {
    "clean-modules": "rimraf node_modules",
    "clean": "rimraf dist npm-debug.log* yarn-error.log*",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "eslint \"src/**/*.ts\" \"src/**/*.tsx\" --ext ts --ext tsx --ext js",
    "check:types": "tsc --pretty --noEmit",
    "test:all": "yarn check:types && yarn format:check && yarn lint",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": "tsc --pretty --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-select": "^5.0.1",
    "@types/react-slick": "^0.23.8",
